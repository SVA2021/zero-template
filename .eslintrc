{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-console": "warn",
        "quotes": [
            "warn",
            "single"
        ],
        "jsx-quotes": [
            "warn",
            "prefer-double"
        ],
        "prefer-const": "error",
        "max-len": [
            "warn",
            {
                "code": 120
            }
        ],
        "comma-dangle": [
            "warn",
            "always-multiline"
        ]
    }
}